@{ Layout = null;}
@using WoBangMai.Utils;
@model IEnumerable<WoBangMai.Models.cms_category>

<div class="side_box has_sub">
    <div class="section_title_1">
        <p>作品/GALLERY</p>
    </div>
    <div class="catg_name">
        @if (Model != null && Model.Count() > 0)
        {
            foreach (var item in Model)
            {
                <a href="/Gallery?id=@item.Category_ID" class="@(item.Category_ID==ZConvert.ToInt(TempData["currentchildNav"])?"on":"off")" >@item.CategoryName</a>
            }
        }
        else
        {
            <a href="/Gallery" class="on">平面模特摄影</a>
            <a href="/Gallery">国外主题摄影作品</a>
        }

    </div>
</div>