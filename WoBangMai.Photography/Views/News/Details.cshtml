@model  WoBangMai.Models.cms_news
@{
    ViewBag.Title = "Index";
    Layout = null;
    var ajaxOption = new AjaxOptions()
    {
        HttpMethod = "Post",
        UpdateTargetId = "tb001",
        OnBegin = "$('#pageMode_tableConetnt').mask();",
        OnComplete = "$('#pageMode_tableConetnt').unmask();",
        InsertionMode = InsertionMode.Replace
    };
}
<!DOCTYPE html>
<html lang="zh-cn">

<!-- Mirrored from by Maker Cloud Website www.va666.com -->
<head>
    <meta charset="utf-8">
    <meta content="telephone=no" name="format-detection" />
    <meta content="email=no" name="format-detection" />
    <meta name="apple-mobile-web-app-capable" content="yes" />
    <meta name="generator" content="abcmoban.com">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, minimum-scale=1.0, maximum-scale=1.0, user-scalable=no" />
    <title>@(Model == null ? "" : Model.NewsTitle)</title>
    <meta name="description" content="@(Model==null?"":Model.NewsTitle)">
    <meta name="keywords" content="@(Model==null?"":Model.NewsTitle)">
    <link rel="shortcut icon" href="favicon.html" />
    <link rel="stylesheet" href="/assets/css/style.css">
    <link rel="stylesheet" href="/assets/css/slick.css">
    <link href="/assets/css/discuss.css" rel="stylesheet" />
    <style type="text/css">
       a, .section_title_1 .setting span, a.more, .side_box .section_title_1 > p, .form .section_title_1 > p, .catg_hide p, .list_text a:hover p, .catg_name a:hover, .foot_extend a:hover, .gallery_box:hover span { color: #48b4c2;}
       .section_title_1 .setting span:after, .btn_1 a:hover, .catg_hide, a.btn_contact:hover, .form input:focus, .form textarea:focus {border-color: #48b4c2; }
       .home .section_title_3 > p:after, .home .section_title_1 > p:after, .slick-dots li.slick-active button, .alpha_bg, .btn_1 a:after, .gallery_box:hover:before, .btn_2 a, .catg_name a.on, a.btn_2:before, .page_on a, a.btn_contact:hover, .formBtn button:hover, .service_box_text p a:hover { background-color: #48b4c2 }
       .header { background-color: #222; }
       .nav li a, .header_info p, .header_info p a {color: #f0f0f0;}
       .on a { border-color: #f0f0f0;}
    </style>
    <script src="/assets/js/jquery1.9.js"></script>
    <script src="/assets/js/slick.min.js"></script>
    <script src="/assets/js/scrollReveal.js"></script>
    <script src="/assets/js/main.js"></script>
    <!--[if IE 8]>
    <script src="http://h09b.abcmoban.com/assets/js/background-size.js"></script>	<script src="http://h09b.abcmoban.com/assets/js/selectivizr.js"></script>	<![endif]-->
</head>
<body>
    <div class="second_page_b">
        <!-- <img src="images/b1.jpg"> -->
    </div>

    @*导航*@
    @{Html.RenderAction("_Nav", "Tools");}

    <div class="h_100"></div>
    <div class="content">
        <div class="second_page">
            <div class="side_l">
                <!-- 画廊 -->
                @{Html.RenderAction("_GalleryNewsSide", "Tools");}

                @*联系我们*@
                @{Html.RenderAction("_ContactNewsSide", "Tools");}
            </div>
            <div class="side_r">
                @if (Model != null)
                {
                    <div class="detail_time">@(Model.CreateTime == null ? DateTime.Now.ToString("yyyy/MM/dd") : ((DateTime)Model.CreateTime).ToString("yyyy/MM/dd"))</div>
                    <div class="title_3">@(Model.NewsTitle)</div>
                    <div class="detail_text">
                        @Html.Raw(Model.NewsContent)
                    </div>
                            <div class="ctrl">
                                <a class="btn_2" id="back" href="/News"></a>
                                <a class="btn_2" id="next" href="-news-2.html"></a>
                                <div class="clear"></div>
                            </div>

                }
                else
                {
                    <div class="detail_time">2014/11/04</div>
                    <div class="title_3">春季联名款全面呈现</div>
                    <div class="detail_text">
                        <img src="/content/2-news/1--news-1/5.jpg">
                        <p>为空默认显示</p>
                        <img src="/content/image/01.jpg" />
                    </div>
                            <div class="ctrl">
                                <a class="btn_2" id="back" href="/News"></a>
                                <a class="btn_2" id="next" href="-news-2.html"></a>
                                <div class="clear"></div>
                            </div>
                }

                @*评论*@
                @{Html.RenderAction("_Comment", "Tools", new { Id = Model.News_ID });}
            </div>
        </div><!-- second_page-end -->


    </div><!-- content-end -->
    @*底部*@
    @{Html.RenderAction("_Footer", "Tools");}
    <script type='text/javascript' src='/assets/js/smoothscroll.js'></script>
</body>
<!-- Mirrored from by Maker Cloud Website www.va666.com -->
</html>
