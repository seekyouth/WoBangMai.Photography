@{
    ViewBag.Title = "Index";
    Layout = null;
    var lastNexNewsList = ViewBag.lastNexNewsList as List<WoBangMai.Models.News>;
    var otherNewsList = ViewBag.otherNewsList as List<WoBangMai.Models.News>;
    var categoryList = ViewBag.categoryList as List<WoBangMai.Models.Category>;
}
@model  WoBangMai.Models.News
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml" class="agent-pc theme-w theme-wblue">
<head>
    <meta charset="utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
    <meta name="keywords" content="e:field name='keywords'/}" />
    <meta name="description" content="" />
    <meta name="author" content="Maker Cloud" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=0" />
    <meta content="yes" name="apple-mobile-web-app-capable" />
    <meta content="black" name="apple-mobile-web-app-status-bar-style" />
    <meta content="telephone=no" name="format-detection" />
    <link href="~/Content/Css/lib2.css" rel="stylesheet" />
    <link href="~/Content/Css/style.base.css" rel="stylesheet" />
    <link href="~/Content/Css/wblue.css" rel="stylesheet" />
    <link href="~/Content/Css/base.css" rel="stylesheet" />
    <script src="~/Scripts/jquery.min.js"></script>
    <script src="~/Scripts/org.base.js" data-main="baseMain"></script>
    <script src="~/Scripts/Base.js"></script>
    <script type="text/javascript">if (window.location.toString().indexOf('pref=padindex') != -1) { } else { if (/AppleWebKit.*Mobile/i.test(navigator.userAgent) || (/MIDP|SymbianOS|NOKIA|SAMSUNG|LG|NEC|TCL|Alcatel|BIRD|DBTEL|Dopod|PHILIPS|HAIER|LENOVO|MOT-|Nokia|SonyEricsson|SIE-|Amoi|ZTE/.test(navigator.userAgent))) { if (window.location.href.indexOf("?mobile") < 0) { try { if (/Android|Windows Phone|webOS|iPhone|iPod|BlackBerry/i.test(navigator.userAgent)) { window.location.href = "/m/list.php?tid=2"; } else if (/iPad/i.test(navigator.userAgent)) { } else { } } catch (e) { } } } }</script>
    <title>@Model.Title??""</title>
</head>
<body id="longPage" class="gh0  longPage   bodyservice ">
    <div class="bodyMask"></div>
    <div id="header">
        <div class="wrapper">
            <div class="content">
                <div id="headTop">
                    <a href="/" id="logo"><img src="/Content/Picture/logo.png" height="44"></a>
                </div>
                <div id="navWrapper">
                    @{Html.RenderAction("_Nav", "Part");}
                </div>
            </div>
        </div>
    </div>
    <div id="sitecontent" class="ff_sitecontent">
        <div class="npagePage post ff_postPage">
            <div id="pageTarget" class="ff_pageTarget module">
                <div class="module_container">
                    <div class="clear"></div>
                </div>
            </div>
            <div class="content">
                <div class="mlistpost service module">
                    <div class="module_container">
                        <div class="container_content">
                            <div class="content_wrapper">
                                <div id="postWrapper">
                                    <div class="postContent">
                                        <div class="postInfo">
                                            <div class="mainInfor">
                                                <p class="title">@Model.Title??""</p>
                                                @*<p class="subtitle">Intelligence</p>*@
                                                <p class="usetdate"> 
                                                <span class="year">@Model.AddTime.Year</span>
                                                <i class="time-connect">-</i><span class="m">@Model.AddTime.Month</span>
                                                <i class="time-connect">-</i><span class="d">@Model.AddTime.Day</span>
                                                 </p>
                                            </div>
                                            <div class="description">
                                                <p>@Model.ZhaiYao??""</p>
                                                <p><br /></p>
                                            </div>
                                        </div>
                                        <div class="postbody">
                                           @Html.Raw(Model.Content??"")
                                        </div>
                                        <div class="conTabBtn tabBtn">
                                            @if (lastNexNewsList != null)
                                            {
                                                foreach (var item in lastNexNewsList)
                                                {
                                                    <a href="/Product/Details/@item.Id" class="post-prev">
                                                        <div class="img-wrap">
                                                            <img src="@item.ImgUrl" alt="@item.Title??''" />
                                                        </div>
                                                        <div class="infor-wrap">
                                                            <span class="title">item.Title??''</span>
                                                            @*<span class="subtitle">Wool scarf</span>*@
                                                        </div> <i class="details"></i>
                                                        <div class="tabMask"></div>
                                                    </a>
                                                }
                                            }
                                        </div>
                                    </div>
                                    <div class="listWrap">
                                        <div id="listContent" class="listContent_post">
                                            <h3>相关内容</h3>
                                            <div class="item_tags">
                                                @if (categoryList != null)
                                                {
                                                    foreach (var item in categoryList)
                                                    {
                                                        <a href="/Case/Index/@item.Id" target="_blank">@item.Title??""</a>
                                                    }
                                                }
                                            </div>
                                            <div class="mlist type04">
                                                <div class="content_wrapper">
                                                    <div class="content_list clearfix">
                                                        @if (otherNewsList != null)
                                                        {
                                                            foreach (var item in otherNewsList)
                                                            {
                                                                <div id="item_block_@item.Id" class="item_block_1 item_block wow" style="animation-delay:.1s">
                                                                    <div class="item_index">
                                                                       @item.Title??""
                                                                    </div>
                                                                    <a href="/News/Details/@item.Id" class="item_box">
                                                                        <div href="/News/Details/@item.Id" class="item_img" target="_blank">
                                                                            <img src="@item.ImgUrl??''" />
                                                                            <div class="item_mask"></div>
                                                                        </div>
                                                                        <div class="item_wrapper clearfix">
                                                                            <div class="item_info clearfix">
                                                                                <div class="date_wrap">
                                                                                    <span class="year">@Model.AddTime.Year</span>
                                                                                    <i class="time-connect">-</i>
                                                                                    <span class="m">@Model.AddTime.Month</span>
                                                                                    <i class="time-connect">-</i>
                                                                                    <span class="d">@Model.AddTime.Day</span>
                                                                                </div>
                                                                                <p class="title ellipsis">空气中到底有哪些污染物</p>
                                                                                <p class="subtitle ellipsis"></p>
                                                                            </div>
                                                                            <div class="date_wrap">
                                                                                <span class="year">@Model.AddTime.Year</span>
                                                                                <i class="time-connect">-</i>
                                                                                <span class="m">@Model.AddTime.Month</span>
                                                                                <i class="time-connect">-</i>
                                                                                <span class="d">@Model.AddTime.Day</span>
                                                                            </div>
                                                                            <div class="item_des">
                                                                                <p class="description">@item.ZhaiYao??""</p>
                                                                            </div>
                                                                        </div>
                                                                        <div class="item_index">
                                                                            @Model.Title??""
                                                                        </div>
                                                                    </a>
                                                                    <a href="/News/Details/@item.Id" class="details"> MORE<i class="fa fa-angle-right" aria-hidden="true"></i> </a>
                                                                    <div class="item_tags clearfix">
                                                                        <i class="fa fa-tags"></i>
                                                                    </div>
                                                                </div>
                                                            }
                                                        }

                                                    </div>
                                                    <a href="" class="more wow" style="animation-delay:.5s">MORE<i class="fa fa-angle-right" aria-hidden="true"></i></a>
                                                </div>
                                                <div class="clear"></div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="clear"></div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    @*底部*@
    @{Html.RenderAction("_Footer", "Part");}
    @*底部*@
    @*分享*@
    @{Html.RenderAction("_Shares", "Part");}
    @*分享*@

    @*定点*@
    @{Html.RenderAction("_Fixed", "Part");}
    @*定点*@

    @*在线跳转*@
    @{Html.RenderAction("_OnlineOpen", "Part");}
    @*在线跳转*@

    @*在线咨询*@
    @{Html.RenderAction("_OnlineLx", "Part");}
    @*在线咨询*@

    @*协助*@
    @{Html.RenderAction("_OpenAssist", "Part");}
    @*协助*@

    @*返回顶部*@
    @{Html.RenderAction("_AssistBtn", "Part");}
    @*返回顶部*@
    <div class="hide">
    </div>
</body>
</html>